{
  "name": "IIROSE",
  "version": "0.0.2",
  "description": "IIROSE",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --debug"
  },
  "build": {
    "productName": "Electron-IIROSE",
    "appId": "com.iirose.electron",
    "copyright": "Theresa",
    "directories": {
      "output": "build"
    },
    "win": {
      "icon": "./icon/256.png",
      "target": [
        {
          "target": "nsis"
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  },
  "keywords": [],
  "author": "Theresa",
  "devDependencies": {
    "electron": "^4.1.0"
  }
}
